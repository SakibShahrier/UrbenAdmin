<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nrahman171291.urbenagent">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.UrbenAgent">
        <activity android:name=".UpdateBusLocation"></activity>
        <activity android:name=".AddOffer" />
        <activity android:name=".RemovePassenger" />
        <activity android:name=".RemoveDriver" />
        <activity android:name=".AddCounter" />
        <activity android:name=".AddTicket" />
        <activity android:name=".AddDriver" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>